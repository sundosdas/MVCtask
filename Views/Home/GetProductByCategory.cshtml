@{
    Layout = "_Resto";
}
<head>
    <title> Products...</title>
</head>
<body>
    <div class="container mt-4">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-sm-6 col-md-4 col-lg-3">
                    <div class="card">
                        <img src="@Url.Content("~/AdminAssets/img/" + item.Namee + ".jpg")" class="card-img-top" alt="Food Image" width="200" height="300">
                        <div class="card-body">
                            
                            <h5 class="card-title">Name: @item.Namee</h5>
                            <p class="card-text"> Price: @item.Price</p>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</body>



